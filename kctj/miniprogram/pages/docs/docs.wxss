.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
  width:40%;
  justify-content: center;
  text-align: center;
}

.in{
  width:40%;
  justify-content: center;
  text-align: center;
  color: rgb(6, 112, 38);
}

.out{
  width:40%;
  justify-content: center;
  text-align: center;
  color: rgb(216, 11, 28);
}
.bg-w{
  background: snow;
}
.bg-g{
  background: snow;
}
.th {
  width: 40%;
  justify-content: center;
  background: rgb(6, 92, 34);
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
