.bg{
  width:100%;
  height: 3rem;
  background: rgb(4, 94, 38);
  border-bottom: 1rpx solid #ccc;
}

.tr {
  width: 90%;
  height: 3rem;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #ccc;
}

.th {
  width: 20%;
  height: 3rem;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.th2 {
  width: 60%;
  height: 3rem;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgb(4, 94, 38);
  color: #fff;
}

.td {
  width:20%;
  display: flex;
  justify-content: center;
  text-align: center;
}

.td2 {
  width:60%;
  display: flex;
  justify-content: center;
  text-align: center;
}

.circle{
  display: flex;
  position: fixed;
  flex-direction: column;
  width: 100rpx;
  height: 100rpx;
  background-color: rgb(4, 97, 9);
  border-radius: 200rpx;
  font-size: 30rpx;
  bottom: 100rpx;
  right: 5rpx;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #ccc;
  color:#fff;
}

.uploader-text{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
